<body id="eventpage">
  <header>
    <h1>WHERE-TO</h1>
  </header>
    <input type="button" name="name" value="All Events">
    <input type="button" name="name" value="Street Fest">
    <input type="button" name="name" value="Movies at park">
  <div id="container">
    <div class="info-content">

      <div class="content">
        <div class="imagePort">
        </div>
        <section id="infosection">
        </section>
      </div>

      <aside class="moreinfo">
      </aside>
      
    </div>

  </div>

  <script type="text/template" id="event-template">

      <img src="<%%= image %>" />
      <h2> <%%= title %> </h2>
      <h4> <%%= moviename %> <span> <%%= movierating %> </span> </h4>
      <p><strong> <%%= startdate %> </strong> <%%= hour %> </p>
      <h4> <%%= neighborhood %> </h4>
      <strong> <%%= location %> </strong>
      <p>
        <%%= description %>
      </p>
      <a href=" <%%= link %> "> <%%= link %> </a>

  </script>

</body>
